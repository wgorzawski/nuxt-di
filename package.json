{
  "name": "nuxt-di",
  "version": "0.1.3",
  "description": "Nuxt 3 IOC with Awilix",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wgorzawski/nuxt-di.git"
  },
  "keywords": [
    "nuxt",
    "nuxt3",
    "ioc",
    "awilix"
  ],
  "bugs": {
    "url": "https://github.com/wgorzawski/nuxt-di/issues"
  },
  "homepage": "https://github.com/wgorzawski/nuxt-di#readme",
  "packageManager": "pnpm@9.10.0",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && husky",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "3.14.1592",
    "awilix": "12.0.3"
  },
  "devDependencies": {
    "@nuxt/devtools": "1.6.0",
    "@nuxt/eslint-config": "0.6.1",
    "@nuxt/module-builder": "0.8.4",
    "@nuxt/schema": "3.14.159",
    "@nuxt/test-utils": "3.14.4",
    "@types/node": "22.9.0",
    "changelogen": "0.5.7",
    "eslint": "9.14.0",
    "husky": "9.1.6",
    "nuxt": "3.14.159",
    "typescript": "5.6.3",
    "vitest": "2.1.4",
    "vue-tsc": "2.1.10"
  }
}
